{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "POSTGRES_URL": "@vercel-postgres-url",
    "GITHUB_PERSONAL_ACCESS_TOKEN": "@github_personal_access_token",
    "GITHUB_REPO_OWNER": "@github_repo_owner",
    "GITHUB_REPO_NAME": "@github_repo_name"
  }
}